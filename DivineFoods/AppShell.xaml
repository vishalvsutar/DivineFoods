<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DivineFoods.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DivineFoods"
    Shell.FlyoutBehavior="Disabled">

  <TabBar>
          <ShellContent
        Title="Home"
        Icon="home.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Nearby Restaurant"
        Icon="map.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Orders"
        Icon="orders.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        </TabBar>

</Shell>

